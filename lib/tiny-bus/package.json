{
  "name": "@ohfc/tiny-bus",
  "version": "0.0.1",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "description": "A tiny, typed event bus for in-process publish/subscribe event handling in browser and Node.js environments.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "keywords": [],
  "author": "m10rten",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "index": [
        "dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "package.json",
    "LICENSE",
    "README.md"
  ]
}
